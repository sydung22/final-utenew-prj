{
    "version": 2,
    "builds": [
      {
        "src": "package.json", 
        "use": "vercel-sapper",
        "config": {
          "serverFiles": ["package.json"]
        },
        "functions": {
          "src/pages/**/*": {
            "memory": 3008
          }
        }
      }
    ],
    "routes": [
      {
        "src": "/sw.js",
        "continue": true,
        "headers": {
          "Cache-Control": "public, max-age=0, must-revalidate",
          "Service-Worker-Allowed": "/"
        }
      }
    ]
}